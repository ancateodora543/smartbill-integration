{
  "name": "smartbill",
  "vendor": "vtexeurope",
  "version": "1.0.1",
  "title": "SmartBill",
  "description": "SmartBill Integration",
  "billingOptions": {
    "termsURL": "https://compliance.vtex.com/gdpr/policies/vtex-privacy-policy",
    "support": {
      "url": "https://support.vtex.com/hc/requests"
    },
    "free": true
  },
  "mustUpdateAt": "2022-08-28",
  "categories": [],
  "dependencies": {},
  "builders": {
    "node": "6.x"
