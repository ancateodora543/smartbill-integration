{
  "name": "smartbill",
  "vendor": "vtexeurope",
  "version": "1.0.0",
  "title": "Smartbill",
  "description": "Smartbill Integration",
  "billingOptions": {
    "termsURL": "https://compliance.vtex.com/gdpr/policies/vtex-privacy-policy",
    "support": {
      "url": "https://support.vtex.com/hc/requests"
    },
    "free": true
  },
  "mustUpdateAt": "2022-08-28",
  "categories": [],
  "dependencies": {},
  "builders": {
    "node": "6.x"
  },
  "scripts": {
    "prereleasy": "bash lint.sh"
  },
  "credentialType": "absolute",
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "ws.smartbill.ro",
        "path": "/SBORO/*"
      }
    }
  ],
  "settingsSchema": {
    "title": "Innoship",
    "type": "object",
    "properties": {
      "smarbillUsername": {
        "title": "Smartbill Username",
        "description": "Enter your smartbill username",
        "type": "string"
      },
      "smarbillApiToken": {
        "title": "Smartbill API TOKEN",
        "description": "Enter your smartbill api token",
        "type": "string"
      },
      "smarbillVatCode": {
        "title": "Smartbill Vat Code",
        "description": "Enter your vat code",
        "type": "string"
      },
      "smarbillSeriesName": {
        "title": "Smartbill Series Name",
        "description": "Enter your series name",
        "type": "string"
      }
    }
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}